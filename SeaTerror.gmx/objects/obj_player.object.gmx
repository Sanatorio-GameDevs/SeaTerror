            <string>///Controllers

//Check_keyboard
key_right = keyboard_check(vk_right);
key_left =  keyboard_check(vk_left);
key_jump =  keyboard_check_pressed(vk_space);

//horizontal_axis
move_direction = (key_right - key_left);
    if (abs(move_direction)) {
        if ((hsp &gt; -max_hsp) || (hsp &lt; max_hsp))
            hsp += move_direction * accel;
        else
            hsp = move_direction * max_hsp;
        last_direction = move_direction;
    }
    else {
        if (hsp != 0) {
            if (last_direction == 1 ) {         //Se estivesse se movendo para direita
                hsp -= water_resistance * mass;
                if (hsp &lt; 0)
                    hsp = 0;
            }
            else if (last_direction == -1) {    //Se estivesse se movendo para esquerda
                hsp += water_resistance * mass;
                if (hsp &gt; 0)
                    hsp = 0;
            }
        }
    }

//vertical_axis
    if (vsp &lt;= max_vsp)
        vsp += grav;
    else
        vsp = max_vsp;
    if (place_meeting(x,y+1,obj_Solid)) {
        if (key_jump)
            vsp = jmp_accel;
    }

//horizontal_collision
    if ((place_meeting(x+hsp,y,obj_Solid)) &amp;&amp; !(place_meeting(x+sign(hsp),y,obj_Movable))) {
        while (!(place_meeting(x+sign(hsp),y,obj_Solid)))
            x += sign(hsp);
        hsp = 0;
    }
    else {
        x += hsp;
    }

    //vertical_collision
    if (place_meeting(x,y+vsp,obj_Solid)) {
        while !(place_meeting(x,y+sign(vsp),obj_Solid))
            y += sign(vsp);
        vsp =0;
    }
    else {
        y += vsp;
    }

</string>
